{
  "name": "@koru-booking/backend",
  "version": "1.0.0",
  "description": "Koru Booking API Backend",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "tsx src/index.ts",
    "build": "tsc",
    "build:vercel": "prisma generate && tsc",
    "vercel-build": "prisma generate && tsc",
    "postdeploy": "prisma migrate deploy",
    "migrate:pre-deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:push": "prisma db push",
    "prisma:seed": "tsx prisma/seed.ts",
    "db:setup": "npm run prisma:generate && npm run prisma:migrate:deploy && npm run prisma:seed",
    "type-check": "tsc --noEmit"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@react-email/components": "^1.0.1",
    "@react-email/render": "^2.0.0",
    "@supabase/supabase-js": "^2.39.0",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.3",
    "react": "^19.2.3",
    "react-email": "^5.0.7",
    "resend": "^6.6.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.0",
    "@types/react": "^19.2.7",
    "prisma": "^5.7.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "koru",
    "booking",
    "api",
    "appointments"
  ],
  "author": "Red Clover",
  "license": "MIT"
}
